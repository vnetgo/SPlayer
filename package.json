    "@vueuse/core": "^12.8.2",
    "NeteaseCloudMusicApi": "^4.27.0",
    "axios": "^1.8.4",
    "change-case": "^5.4.4",
    "dayjs": "^1.11.13",
    "electron-dl": "^4.0.0",
    "electron-store": "^8.2.0",
