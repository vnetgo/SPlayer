    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/http-proxy": "^11.1.2",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.1.1",
